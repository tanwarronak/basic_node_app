<body class="bg-black" ><div class="w-100 vh-auto bg-black d-flex flex-wrap gap-3">
<% userdata.map((items)=>{%><div class="card bg-dark text-white h-25" style="width: 10rem">
  <img src="<%= items.img %>" class="card-img-top" alt="...">
  <div class="card-body">
    <h4 class="card-text"><%=items.name%></h4>
    <p><%=items.email%></p>
  </div>
  <a type="button" href="/Deleteuser/<%=items.name%>" class="btn btn-danger">Delete user</a>
  <a type="button" href="/Updateuser/<%=items.name%>" class="btn btn-warning">Update user</a>
</div><%});%>
</div>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
</body>
</html>